              </div>
            </nav>
          </div>
        </div>
        <!-- /top navigation -->

        <!-- page content -->
        <div class="right_col" role="main">
          <!-- top tiles -->
          <div class="row tile_count">
              <%- include('./components/status.ejs') %>
          </div>
          <!-- /top tiles -->

          <div class="row">
            <%- include('./components/main_chart.ejs') %>
            <%- include('./components/setting.ejs') %>
          </div>
          <br />

          <div class="row">
            <%- include('./components/sub_chart_1.ejs') %>
            <%- include('./components/sub_chart_2.ejs') %>
            <%- include('./components/sub_chart_total.ejs') %>
